{
  "private": true,
  "name": "formation-node-2016-10-10",
  "version": "1.0.0",
  "description": "Quizou",
  "main": "index.js",
  "bin": {
    "formation-node-2016-10-10": "build-quizz-database.js"
  },
  "scripts": {
    "test": "mocha",
    "preclean-client": "echo before cleaning",
    "clean-client": "rm -f public/main.build.js",
    "prebuild-client": "npm run clean-client",
    "build-client": "cross-env NODE_ENV=production browserify -t babelify client/index.js -g uglifyify > public/main.build.js",
    "watch-client": "cross-env NODE_ENV=development watchify -v -d -t babelify client/index.js -o public/main.build.js",
    "prestart": "npm run build-client",
    "start": "cross-env NODE_ENV=production node server",
    "watch-server": "cross-env NODE_ENV=development nodemon -i public -e js,toml server",
    "dev": "npm-run-all -p watch-client watch-server watch-test",
    "watch-test": "mocha --watch -R min --growl"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "cheerio": "^0.22.0",
    "config": "^1.21.0",
    "express": "^4.14.0",
    "fs-promise": "^0.5.0",
    "lodash": "^4.16.4",
    "request": "^2.75.0",
    "request-promise-native": "^1.0.3",
    "toml": "^2.3.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "cross-env": "^3.1.2",
    "eslint": "^3.7.1",
    "eslint-plugin-react": "^6.4.0",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "rewire": "^2.5.2",
    "sinon": "^1.17.6",
    "supertest": "^2.0.0",
    "uglifyify": "^3.0.3",
    "watchify": "^3.7.0"
  }
}
